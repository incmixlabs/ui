{
  "extends": "@incmix/config/react.json",
  "exclude": ["node_modules", "dist", "./*.ts", "./src/demos", "./src/lib"],
  "include": ["src/"],
  "compilerOptions": {
    "types": ["@types/wicg-file-system-access", "vite/client"],
    "rootDir": "src",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["./components/*"],
      "@/components": ["./components"],
      "@/hooks": ["./hooks"],
      "@/hooks/*": ["./hooks/*"],
      "@/icons": ["./components/icons"],
      "@/icons/*": ["./components/icons/*"],
      "@/kanban": ["./components/kanban-board"],
      "@/kanban/*": ["./components/kanban-board/*"],
      "@/layouts": ["./components/layouts"],
      "@/layouts/*": ["./components/layouts/*"],
      "@/lib": ["./lib"],
      "@/lib/*": ["./lib/*"],
      "@/notes": ["./notes"],
      "@/notes/*": ["./notes/*"],
      "@/organizations": ["./organizations"],
      "@/organizations/*": ["./organizations/*"],
      "@/projects": ["./projects"],
      "@/projects/*": ["./projects/*"],
      "@/primitives/*": ["./primitives/*"],
      "@/primitives": ["./primitives"],
      "/projects/*": ["./components/projects/*"],
      "/projects": ["./components/projects"],
      "@/sidebar": ["./components/sidebar"],
      "@/sidebar/*": ["./components/sidebar/*"],
      "@/secondary-sidebars": ["./components/secondary-sidebars"],
      "@/secondary-sidebars/*": ["./components/secondary-sidebars/*"],
      "@/style/*": ["./style/*"],
      "@/style": ["./style"],
      "@/types": ["./types", "./types.ts"],
      "@/types/*": ["./types/*"],
      "@/utils": ["./lib/utils"],
      "@/utils/*": ["./lib/utils/*"],
      "@widgets/*": ["./widgets/*"],
      "@widgets": ["./widgets"]
    }
  }
}
