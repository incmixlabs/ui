{
  "name": "@incmix/pages",
  "version": "0.0.0",
  "private": true,
  "description": "Auth pages package for Incmix",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./i18n": "./src/i18n/index.ts",
    "./auth": "./src/auth/index.ts",
    "./common": "./src/common/index.ts",
    "./dashboard": "./src/dashboard/index.ts",
    "./layouts": "./src/layouts/index.ts",
    "./orgs": "./src/orgs/index.ts",
    "./tasks": "./src/tasks/index.ts",
    "./users": "./src/users/index.ts"
  },
  "scripts": {
    "build": "pnpm clean && echo 'Skipping build' && exit 0",
    "clean": "rm -rf dist *.tsbuildinfo",
    "tsc": "pnpm clean && mv ../ui/package.json ../ui/package-fihi.json && mv ../ui/package-pages.json ../ui/package.json && tsc -p tsconfig.json --outDir dist && mv ../ui/package.json ../ui/package-pages.json && mv ../ui/package-fihi.json ../ui/package.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@casl/react": "catalog:",
    "@incmix/store": "workspace: *",
    "@incmix/ui": "workspace: *",
    "@incmix/utils": "catalog:",
    "@radix-ui/themes": "catalog:",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-router": "catalog:",
    "@tanstack/react-table": "catalog:",
    "@tanstack/zod-form-adapter": "catalog:",
    "i18next": "24.2.1",
    "i18next-chained-backend": "4.6.2",
    "i18next-http-backend": "3.0.1",
    "i18next-resources-to-backend": "1.2.1",
    "immer": "catalog:",
    "lucide-react": "catalog:",
    "nanoid": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "15.4.0",
    "rxdb": "catalog:",
    "rxdb-hooks": "catalog:",
    "swapy": "0.4.2",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "1.0.7",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@incmix/config": "workspace: *",
    "@tanstack/router-cli": "1.37.0",
    "@tanstack/router-devtools": "1.36.3",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/wicg-file-system-access": "2023.10.5"
  }
}
