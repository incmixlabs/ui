{
  "extends": "@incmix/config/react.json",
  "exclude": ["node_modules"],
  "compilerOptions": {
    "types": ["vite/client"],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@hooks": ["./hooks/"],
      "@hooks/*": ["./hooks/*"],
      "@local": ["./local"],
      "@local/*": ["./local/*"],
      "@services": ["./services"],
      "@services/*": ["./services/*"],
      "@sql": ["./sql"],
      "@sql/*": ["./sql/*"],
      "@tabs": ["./tabs"],
      "@tabs/*": ["./tabs/*"]
    }
  }
}
