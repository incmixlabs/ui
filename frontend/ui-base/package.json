{
  "name": "@incmix/ui-base",
  "version": "3.2.4",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "style": "./src/styles/styles.css",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./base/*": {
      "require": {
        "types": "./dist/cjs/base/*.d.ts",
        "default": "./dist/cjs/base/*.js"
      },
      "import": {
        "types": "./dist/esm/base/*.d.ts",
        "default": "./dist/esm/base/*.js"
      }
    },
    "./blocks/*": {
      "require": {
        "types": "./dist/cjs/blocks/*.d.ts",
        "default": "./dist/cjs/blocks/*.js"
      },
      "import": {
        "types": "./dist/esm/blocks/*.d.ts",
        "default": "./dist/esm/blocks/*.js"
      }
    },
    "./elements/*": {
      "require": {
        "types": "./dist/cjs/elements/*.d.ts",
        "default": "./dist/cjs/elements/*.js"
      },
      "import": {
        "types": "./dist/esm/elements/*.d.ts",
        "default": "./dist/esm/elements/*.js"
      }
    },
    "./pages/*": {
      "require": {
        "types": "./dist/cjs/pages/*.d.ts",
        "default": "./dist/cjs/pages/*.js"
      },
      "import": {
        "types": "./dist/esm/pages/*.d.ts",
        "default": "./dist/esm/pages/*.js"
      }
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": ["*.css"],
  "license": "MIT",
  "files": [
    "src/**",
    "dist/**",
    "tokens/**",
    "layout/**",
    "*.css",
    "postcss-breakpoints.cjs",
    "postcss-whitespace.cjs"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json --outDir dist && mv dist/src/* dist/ && rmdir dist/src",
    "typecheck": "tsc --traceResolution",
    "dev": "pnpm run build && pnpm watch & pnpm storybook",
    "watch": "tsc -p tsconfig.json --outDir dist --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "catalog:",
    "@incmix/store": "workspace: *",
    "@radix-ui/react-accordion": "catalog:",
    "@radix-ui/react-dialog": "catalog:",
    "@radix-ui/react-label": "catalog:",
    "@radix-ui/react-menubar": "catalog:",
    "@radix-ui/react-navigation-menu": "catalog:",
    "@radix-ui/react-slot": "catalog:",
    "@radix-ui/react-tabs": "catalog:",
    "@radix-ui/react-toggle": "catalog:",
    "@radix-ui/react-toggle-group": "catalog:",
    "classnames": "^2.3.2",
    "cmdk": "catalog:",
    "date-fns": "catalog:",
    "input-otp": "catalog:",
    "react-day-picker": "^9.8.1",
    "react-hook-form": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@incmix/config": "workspace: *",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-onboarding": "9.1.2",
    "@storybook/react-vite": "9.1.2",
    "autoprefixer": "catalog:",
    "esbuild": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-jsx-a11y": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-storybook": "9.1.2",
    "globals": "catalog:",
    "postcss": "catalog:",
    "postcss-cli": "catalog:",
    "postcss-combine-duplicated-selectors": "catalog:",
    "postcss-custom-media": "catalog:",
    "postcss-discard-empty": "catalog:",
    "postcss-import": "catalog:",
    "postcss-nesting": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "storybook": "9.1.2",
    "stylelint": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "homepage": "https://radix-ui.com/themes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incmix/ui-base.git"
  },
  "bugs": {
    "url": "https://github.com/incmix/ui-base/issues"
  }
}
