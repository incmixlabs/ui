{
  "extends": "@incmix/config/react.json",
  "include": ["src/**/*" ],
  "exclude": ["node_modules", "dist", "build", "coverage", "storybook-static", ".next", ".cache", "stories", "shadcn", "src/preset.ts"],
  "compilerOptions": {
    "types": ["@types/wicg-file-system-access", "vite/client"],
    "paths": {
      "@/src": ["./src"],
      "@/src/*": ["./src/*"],
      "@/base": ["./src/base"],
      "@/base/*": ["./src/base/*"],
      "@/radix-ui": ["./radix-ui/components"],
      "@/radix-ui/*": ["./radix-ui/components/*"],
      "@/shadcn": ["./shadcn"],
      "@/shadcn/*": ["./shadcn/*"]
    }
  }
}
