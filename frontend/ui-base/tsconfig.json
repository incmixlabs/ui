{
  "extends": "@incmix/config/react.json",
  "include": ["src/"],
  "compilerOptions": {
    "types": ["@types/wicg-file-system-access", "vite/client"],
    "rootDir": "src",
    "baseUrl": "./src",
    "paths": {
      "@radix-ui": ["./radix-ui"],
      "@radix-ui/*": ["./radix-ui/*"],
      "@shadcn": ["./shadcn"],
      "@shadcn/*": ["./shadcn/*"]
    }
  },
  "exclude": ["**/*.stories.tsx", ".storybook/**/*", "src/stories/**/*"]
}
